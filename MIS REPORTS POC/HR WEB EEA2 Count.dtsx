            "SELECT [ENTRY_ID] " +
            "FROM [dbo].[" + Dts.Variables["User::Query_Loop_Data_TargetTable"].Value.ToString() + "_H] " +
            "where ENTRY_ID = '" + Dts.Variables["User::Component_Key"].Value.ToString().Trim() + "'" +
