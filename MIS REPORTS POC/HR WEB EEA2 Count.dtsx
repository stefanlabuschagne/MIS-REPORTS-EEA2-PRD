        HTML_Data = Regex.Replace(XMLData, replace_values, "TD", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<Total>", "<TD>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "</Total>", "</TD>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<row>", "<TR>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "</row>", "</TR>" + Environment.NewLine, RegexOptions.IgnoreCase);



        HTML_Data = Regex.Replace(HTML_Data, "</ROOT>", "</TABLE>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<ROOT>", "", RegexOptions.IgnoreCase);


        string CC = Variables.ComponentDescription.ToString().Trim().TrimEnd().TrimStart();
        string DaYear = Variables.FlagFile.Substring(0, 4).ToString();


        // Format the SubTotals - 13 April 2018
        HTML_Data = Regex.Replace(HTML_Data, "zTotal:", "Total:", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<TD>Total:</TD><TD>Total:</TD><TD>Total:</TD><TD>Total:</TD><TD>Total:</TD><TD>Total:</TD>", "<TD Class='ST1' colspan=6 >Total:</TD>", RegexOptions.IgnoreCase);
