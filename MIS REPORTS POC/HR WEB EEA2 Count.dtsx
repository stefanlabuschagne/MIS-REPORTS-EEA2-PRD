        ''''''''''''''''''''''
        ' Add your code here '
        ''''''''''''''''''''''

        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' SAVE THE ComponentID of the Item                                       '
        ' So it can be used as a parentID when the LOWER TREE STRUCTURE IS BUILT '
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        Variables.DaNextComponetsParent = daComponentID.ToString()


    End Sub

    Public Overrides Sub CreateNewOutputRows()

        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' THIS BUILDS UP THE TREESTRUCTURE FOR THE FLAG THAT WAS READ                          '
        ' ITS JUST THE ONE PATH FORM TOP TO BOITTOM,                                           '
        ' AS GUIDED BY THE INFORMATION EXTRACTED FROM THE FLAG and the BUSINESS_UNIT_DIMENSION '
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' 11 May 2016                                            '
        ' We need to know of this flag is "Entity" or "RSA" Item '
        ' This must be looked-up in the Business Unit Dimension  '
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        Dim IsAnEntity As Boolean = True

        If Variables.ComponentIsEntity = "Y" Then
            IsAnEntity = True
        Else
            IsAnEntity = False
        End If


        '' MORE TOPPER ENTITIES
        Output0Buffer.AddRow()
        Output0Buffer.COMPONENT = "Labour"
        Output0Buffer.PARENTCOMPONENT = ""  '' DIS DIE HOOGSTE COMPONENT!
        Output0Buffer.COMPONENTDESCRIPTION = "Labour"
        Output0Buffer.LEVEL = 0

        '' Top - 1 Level
        If IsAnEntity Then

            Output0Buffer.AddRow()
            Output0Buffer.COMPONENT = "ent"
            Output0Buffer.PARENTCOMPONENT = "Labour"
            Output0Buffer.COMPONENTDESCRIPTION = "OTHER ENTITIES"
            Output0Buffer.LEVEL = 1
        Else
            Output0Buffer.AddRow()
            Output0Buffer.COMPONENT = "rsa"
            Output0Buffer.PARENTCOMPONENT = "Labour"
            Output0Buffer.COMPONENTDESCRIPTION = "RSA"
            Output0Buffer.LEVEL = 1
        End If


        '' Top - 2 Level
        If IsAnEntity Then


            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_prov"
                Output0Buffer.PARENTCOMPONENT = "ent"
                Output0Buffer.COMPONENTDESCRIPTION = "Provinces"   '' LAST OF THE MOHICANS!
                Output0Buffer.LEVEL = 2

            Else

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_dept"
                Output0Buffer.PARENTCOMPONENT = "ent"
                Output0Buffer.COMPONENTDESCRIPTION = "National"
                Output0Buffer.LEVEL = 2

            End If


        Else

            '' SPLIT PATH to "Prov" or "Dept" depending on the SHORTCODE

            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "prov"
                Output0Buffer.PARENTCOMPONENT = "rsa"
                Output0Buffer.COMPONENTDESCRIPTION = "Provinces"
                Output0Buffer.LEVEL = 2

            Else

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "dept"
                Output0Buffer.PARENTCOMPONENT = "rsa"
                Output0Buffer.COMPONENTDESCRIPTION = "National"
                Output0Buffer.LEVEL = 2

            End If

        End If

        '' Level 3
        '' ADD THE INCOMING "PROVINCE NAME" OR "NATIONAL DEPARTMENT"

        If IsAnEntity Then


            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_" & Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'   ''STORE THE PROVINCE CODEeg 'ecp' 'nop' etc.
                Output0Buffer.PARENTCOMPONENT = "ent_prov"
                Output0Buffer.COMPONENTDESCRIPTION = Variables.NatProvDescription.ToString()   '' EASTERN CAPE ''  ->> Where do we get this from??
                Output0Buffer.LEVEL = 3

            Else

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_nat"
                Output0Buffer.PARENTCOMPONENT = "ent_dept"
                Output0Buffer.COMPONENTDESCRIPTION = Variables.NatProvDescription.ToString() '' "National Departments" ->> Where do we get this from??
                Output0Buffer.LEVEL = 3

            End If

        Else

            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'   ''STORE THE PROVINCE CODEeg 'ecp' 'nop' etc.
                Output0Buffer.PARENTCOMPONENT = "prov"
                Output0Buffer.COMPONENTDESCRIPTION = Variables.NatProvDescription.ToString()   '' EASTERN CAPE ''  ->> Where do we get this from??
                Output0Buffer.LEVEL = 3

            Else

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "nat"
                Output0Buffer.PARENTCOMPONENT = "dept"
                Output0Buffer.COMPONENTDESCRIPTION = Variables.NatProvDescription.ToString() '' "National Departments" ->> Where do we get this from??
                Output0Buffer.LEVEL = 3

            End If

        End If


        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ''  This is the First 2 Digits of the Year (Read It From The Flag) ''
        ''  Forms part of the Identfier                                    ''
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        Dim YearPrefix As String = Variables.FlagFile.Substring(2, 2).ToString().Trim()  ''  "14"   "15"   "16"
        Dim COMPONENTDESCRIPTION As String = ""

        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' SL 19 April 2018                                              '
        ' If the FlagFile's Month < 10 then we select the Previous Year '
        '  201701 -> 2016-2017   (yearprefix = 16)                      '
        '  201711 -> 2017-2018   (yearprefix = 17)                      '
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        If Variables.FlagFile.Substring(4, 2).ToString().Trim() >= "10" Then
            YearPrefix = Variables.FlagFile.Substring(2, 2).ToString().Trim()
            COMPONENTDESCRIPTION = Variables.FlagFile.Substring(0, 4).ToString().Trim() & " - " & Convert.ToString(Convert.ToInt32(Variables.FlagFile.Substring(0, 4).ToString().Trim()) + 1).Trim()
        Else
            YearPrefix = Convert.ToString(Convert.ToInt32(Variables.FlagFile.Substring(2, 2).ToString().Trim()) - 1).Trim
            COMPONENTDESCRIPTION = Convert.ToString(Convert.ToInt32(Variables.FlagFile.Substring(0, 4).ToString().Trim()) - 1).Trim & " - " & Variables.FlagFile.Substring(0, 4).ToString().Trim()
        End If




        '' ADD THE INCOMING YEAR - Under the Province name

        If IsAnEntity Then


            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_" + YearPrefix + "_" + Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'   ->>  15_ecp
                Output0Buffer.PARENTCOMPONENT = "ent_" + Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'
                Output0Buffer.COMPONENTDESCRIPTION = COMPONENTDESCRIPTION '' ** YEAR AS DISPLAYED ON THE TREE **
                Output0Buffer.LEVEL = 4

            Else '' DIT HET NIE N UNDERSCORE NIE SOOS DIS "NATIONAL" DEPARTMENT

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = "ent_" + YearPrefix + "_" + "nat"  '' Hardcoded 100% -    15_nat
                Output0Buffer.PARENTCOMPONENT = "ent_" + "nat"
                Output0Buffer.COMPONENTDESCRIPTION = COMPONENTDESCRIPTION '' ** YEAR AS DISPLAYED ON THE TREE **
                Output0Buffer.LEVEL = 4

            End If

        Else


            If Variables.ShortName.Contains("_") Then

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = YearPrefix + "_" + Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'   ->>  15_ecp
                Output0Buffer.PARENTCOMPONENT = Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'
                Output0Buffer.COMPONENTDESCRIPTION = COMPONENTDESCRIPTION  '' ** YEAR AS DISPLAYED ON THE TREE **
                Output0Buffer.LEVEL = 4


            Else '' DIT HET NIE N UNDERSCORE NIE SOOS DIS "NATIONAL" DEPARTMENT

                Output0Buffer.AddRow()
                Output0Buffer.COMPONENT = YearPrefix + "_" + "nat"  '' Hardcoded 100% -    15_nat
                Output0Buffer.PARENTCOMPONENT = "nat"
                Output0Buffer.COMPONENTDESCRIPTION = COMPONENTDESCRIPTION '' ** YEAR AS DISPLAYED ON THE TREE **
                Output0Buffer.LEVEL = 4



            End If

        End If


        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        '' Following is the Important Level
        '' Level 5 that generates Reports as on Level "99"
        '' These reporst will be be Generated by the HL-Reports Routine
        '' What is important here is the Component Key of this level, that needs to be Passed to the First Lower Level Report
        '' So there is a link between the Department's Report and the rest of the Treestructure
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        '' ADD THE ACTUAL "NATIONAL DEPARTMENT NAME" OR "PROVINCIAL DEPARTMENT NAME"
        '' THIS IS FED FROM A VARIABLE as READ from the Business_Unit_Dimension
        '' THIS COMES FROM THE BUSINESS_UNIT TABLE THAT WAS LOOKED UP WAY BEFORE -- (BUSINESS_UNIT TABLE)
        ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' 13 May 2016                                                               '
        ' The Department / Component Id doers not get a "ent_" In front.            '
        ' They Just egt normal Department Names. But They either needs to be linked '
        ' to a parent with an "ent_" or without none!                               '
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        '' The YearPrefix was already adjusted here for the October Lap (201610) -> 2016-2017
        ''                                                              (201609) -> 2015-2016


        '' Here we save the Componet's ID to be linked with the bottom Half of the TABLE STRUCTURE ''
        daComponentID = YearPrefix + "_" + Variables.ShortName.ToString()

        If IsAnEntity Then

            Output0Buffer.AddRow()
            If Variables.ShortName.Contains("_") Then   '' EG ecp_heal -> Thus Provicial Department
                Output0Buffer.COMPONENT = daComponentID '' YEAR + SHORTNAME eg 14_ECP_HEAL
                Output0Buffer.PARENTCOMPONENT = "ent_" + YearPrefix + "_" + Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal'   LINK IT TO AN ENTITY!!!
                Output0Buffer.COMPONENTDESCRIPTION = Variables.DeptDes  '' As read From The BUSINESS_UNIT_DIMENSION
                Output0Buffer.LEVEL = 99
            Else '' DIT HET NIE N UNDERSCORE NIE SOOS DIS "NATIONAL" DEPARTMENT
                Output0Buffer.COMPONENT = daComponentID  '' YEAR + SHORTNAME (Natiional Department SC)
                Output0Buffer.PARENTCOMPONENT = "ent_" + YearPrefix + "_" + "nat"  '' THIS ALSO NEEDS TO BE READ FROM SOMEWHERE
                Output0Buffer.COMPONENTDESCRIPTION = Variables.DeptDes  '' As read From The BUSINESS_UNIT_DIMENSION
                Output0Buffer.LEVEL = 99
            End If

        Else

            '' Here we save the Componet's ID to be linked with the bottom Half of the TABLE STRUCTURE ''
            daComponentID = YearPrefix + "_" + Variables.ShortName.ToString()

            Output0Buffer.AddRow()
            If Variables.ShortName.Contains("_") Then   '' EG ecp_heal -> Thus Provicial Department
                Output0Buffer.COMPONENT = daComponentID '' YEAR + SHORTNAME eg 14_ECP_HEAL
                Output0Buffer.PARENTCOMPONENT = YearPrefix + "_" + Variables.ShortName.Trim.Substring(0, 3) '' "ECP"  van die 'ecp_heal' 
                Output0Buffer.COMPONENTDESCRIPTION = Variables.DeptDes  '' As read From The BUSINESS_UNIT_DIMENSION
                Output0Buffer.LEVEL = 99
            Else '' DIT HET NIE N UNDERSCORE NIE SOOS DIS "NATIONAL" DEPARTMENT
                Output0Buffer.COMPONENT = daComponentID  '' YEAR + SHORTNAME (Natiional Department SC)
                Output0Buffer.PARENTCOMPONENT = YearPrefix + "_" + "nat"  '' THIS ALSO NEEDS TO BE READ FROM SOMEWHERE
                Output0Buffer.COMPONENTDESCRIPTION = Variables.DeptDes  '' As read From The BUSINESS_UNIT_DIMENSION
                Output0Buffer.LEVEL = 99
            End If

        End If

    End Sub

End Class
]]></arrayElement>
