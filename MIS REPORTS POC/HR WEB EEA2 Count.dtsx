            COMPONENTDESCRIPTION = Dts.Variables("User::Flag_File").Value.ToString.Substring(0, 4).ToString().Trim() & " - " & Convert.ToString(Convert.ToInt32(Dts.Variables("User::Flag_File").Value.Substring(0, 4).ToString().Trim()) + 1).Trim()
        Else
            COMPONENTDESCRIPTION = Convert.ToString(Convert.ToInt32(Dts.Variables("User::Flag_File").Value.ToString.Substring(0, 4).ToString().Trim()) - 1).Trim & " - " & Dts.Variables("User::Flag_File").Value.Substring(0, 4).ToString().Trim()
        End If


        Select Case Dts.Variables("User::Query_Loop_Data_SubsetBy").Value.ToString.Trim
            Case "[EEA2_Promotions]"

                '' Filter on the EEA2_Year  to be "xxxx/xxxx" not  "xxxx - xxxx"
                _EEA2_Year_ = "[EEA2_Year] = '" & COMPONENTDESCRIPTION.ToString.Trim.Replace(" - ", "/").ToString() & "'"
                Query_DBC = Query_DBC_L5_PROMOTIONS
                '' Query_H = Query_DBC_L5_PROMOTIONS

            Case "[HeadCount_PostCount]"

                '' Filter on the EEA2 YEAR
                '' We need to FILTER FOR THIS YEAR AND THE NEXT
                '' Thats Because we need to calc the totals of the Next October in the same line SL - 24 April 2018
                '' THEREFOR WE HAVE A ">=" and not an "=" alone
                _EEA2_Year_ = " Case         " & ControlChars.CrLf &
                            " when  right([YearMonth],2) <= '09' then cast(([Year]-1) as varchar(4)) + '/' + cast([Year] as varchar(4))                                                                                                                                    " & ControlChars.CrLf &
                            "    Else  cast([Year] As varchar(4)) + '/' + cast(([Year]+1) as varchar(4) )                                                 " & ControlChars.CrLf &
                            " End >=  '" & COMPONENTDESCRIPTION.ToString.Trim.Replace(" - ", "/").ToString() & "'"

                '' Later on we need to filter out the first year so out totals reflect correct
                '' THEREFORE we need this year_variable on its own
                _EEA2_Year_Filter_ = COMPONENTDESCRIPTION.ToString.Trim.Replace(" - ", "/").ToString()

                Query_DBC = Query_DBC_L5_PIVOT
                '' Query_H = Query_DBC_L5_PIVOT


            Case "[EEA2_Appointments]"

                '' Filter on the EEA2_Year  to be "xxxx/xxxx" not  "xxxx - xxxx"
                _EEA2_Year_ = "[EEA2_Year] = '" & COMPONENTDESCRIPTION.ToString.Trim.Replace(" - ", "/").ToString() & "'"


                'Query_DBC = Query_DBC_L5_Appointments
                Query_DBC = Query_DBC_L5_PROMOTIONS.Replace("[HR_MIS_STAGING].[dbo].[EEA2_Promotions]", " [HR_MIS_STAGING].[dbo].[EEA2_Appointments] ")

            Case "[EEA2_Terminations]"

                '' Filter on the EEA2_Year  to be "xxxx/xxxx" not  "xxxx - xxxx"
                _EEA2_Year_ = "[EEA2_Year] = '" & COMPONENTDESCRIPTION.ToString.Trim.Replace(" - ", "/").ToString() & "'"


                'Query_DBC = Query_DBC_L5_Termminations
                Query_DBC = Query_DBC_L5_PROMOTIONS.Replace("[HR_MIS_STAGING].[dbo].[EEA2_Promotions]", " [HR_MIS_STAGING].[dbo].[EEA2_Terminations] ")


        End Select

        Dim _Ent_Ind_ As String = ""

        If Dts.Variables("User::Component_IsEntity").Value.ToString.Trim() = "N" Then
            _Ent_Ind_ = "N"
        ElseIf True Then
            _Ent_Ind_ = "Y"
        End If

        '' This needs Testing 
        Build_SQL_Statements_UniversalReport(Full_Query_Detail, "", 0, _Select_Structure_, "", _Ent_Ind_, _InnerSelect_Structure_, _GroupBYStructure_.ToString(), _DaYear_.ToString(), _ShortName_, _GroupBYStructureWithoutTotals_, _Select_Structure_With_zTotals_.ToString(), _EEA2_Year_.ToString(), _Component_Level_Field_, _Component_Level_Value_, _EEA2_Year_Filter_)


        '''''''''''''''''''''''''''''''''''''''''''''''''''''
