        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        '' SL 26-07-2018                                                                                                           ''
        ''                                                                                                                         ''
        '' For the Reporting STRUCTURE we need to Query the Structures from OCTOBER the Current Year, Till September the next Year ''
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

        '' This needs attention!!
        Dim _Organisation_ As String = Dts.Variables("User::Dept_Code").Value.ToString()
        '' Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(12, 2)

        Dim Month_Loop_Counter As Integer = 0
        Dim Query As String = ""

        Dim CurrYear As String = ""
        Dim NextYear As String = ""

        '' Determine the Year depending on the value of the month on the flag
        If Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(4, 2).ToString() >= "10" Then
            CurrYear = Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4).ToString()
            NextYear = Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4).ToString() + 1
        Else
            CurrYear = Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4).ToString() - 1
            NextYear = Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4).ToString()
        End If

        '' OCTOBER till December......
        For Month_Loop_Counter = 10 To 12

            Dim _Cur_Month_ As String = (CurrYear) + Right("00" + Month_Loop_Counter.ToString, 2)

            Dim _Month_Begin_ As String = DateSerial(CInt(CurrYear), Month_Loop_Counter, 1).ToShortDateString
            Dim _Month_End_ As String = DateSerial(CInt(CurrYear), Month_Loop_Counter + 1, 0).ToShortDateString

            If Month_Loop_Counter = 10 Then
                Query = Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            Else
                Query = Query & ControlChars.CrLf &
                    "Union All" & ControlChars.CrLf & Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            End If

        Next

        '' JANUARY till September..... THE NEXT YEAR
        For Month_Loop_Counter = 1 To 9

            Dim _Cur_Month_ As String = (NextYear) + Right("00" + Month_Loop_Counter.ToString, 2)

            Dim _Month_Begin_ As String = DateSerial(CInt(NextYear), Month_Loop_Counter, 1).ToShortDateString
            Dim _Month_End_ As String = DateSerial(CInt(NextYear), Month_Loop_Counter + 1, 0).ToShortDateString

            If Month_Loop_Counter = 10 Then
                Query = Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            Else
                Query = Query & ControlChars.CrLf &
                    "Union All" & ControlChars.CrLf & Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            End If

        Next


        '' Original Code tha selkecty the entire structure of 10 000+ rows
        Query = "SELECT     distinct  * from(" + Query + ") as TreeStucture order by 2,3"


        '' JUST FOR TESTING
        '' Query = "SELECT distinct top(10) *  from(" + Query + ") as TreeStucture order by 2,3"
        ''' 
        Debug.Print(Query)


        Dts.Variables("User::Query_Tree_Strucure").Value = Query
        Dts.TaskResult = ScriptResults.Success


    End Sub

