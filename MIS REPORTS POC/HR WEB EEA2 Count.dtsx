        HTML_Data = Regex.Replace(HTML_Data, "<TD>Total:</TD><TD>Total:</TD><TD>Total:</TD>", "<TD Class='ST1' colspan=3 >Total:</TD>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<TD>Total:</TD><TD>Total:</TD>", "<TD Class='ST1' colspan=2 >Total:</TD>", RegexOptions.IgnoreCase);
        HTML_Data = Regex.Replace(HTML_Data, "<TD>Total:</TD>", "<TD Class='ST1' colspan=1 >Total:</TD>", RegexOptions.IgnoreCase);

        // Make the last Total a "GRAND TOTAL:" AND FORMAT THE TOTAL LINES TO A NICE COLOUR
        int DaPlace = HTML_Data.ToString().LastIndexOf("Total:");
        if (DaPlace >= 0)
        {
            HTML_Data = HTML_Data.Insert(DaPlace, "Grand ");

            // Format all the totals up until the end of the rows
            DaPlace = HTML_Data.ToString().IndexOf("Total:");

            while (DaPlace >= 0)
            {

                // Format the rest of the line with the same Class
                DaPlace = HTML_Data.IndexOf("<TD>", DaPlace);                     // Get the position of the next data-cell 
                int End_Of_Row_Posotion = HTML_Data.IndexOf("</TR>", DaPlace);    // Get the end of row position
                string FormatClass = " Class='ST1' ";                             // Formatting-String to Insert

                while ((DaPlace < End_Of_Row_Posotion) && (DaPlace >= 0))
                {
                    DaPlace = DaPlace + 3;
                    HTML_Data = HTML_Data.Insert(DaPlace, FormatClass);
                    End_Of_Row_Posotion = End_Of_Row_Posotion + FormatClass.Length;
                    DaPlace = HTML_Data.IndexOf("<TD>", DaPlace);          // Get the position of the next data-cell    
