        '' This needs attention!!
        Dim _Organisation_ As String = Dts.Variables("User::Dept_Code").Value.ToString()
        '' Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(12, 2)


        Dim Month_Loop_Counter_End As Integer = CInt(Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(4, 2))
        Dim Month_Loop_Counter As Integer = 0
        Dim Query As String = ""

        For Month_Loop_Counter = 1 To Month_Loop_Counter_End

            Dim _Cur_Month_ As String = (Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4)) + Right("00" + Month_Loop_Counter.ToString, 2)

            Dim _Month_Begin_ As String = DateSerial(CInt(Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4)), Month_Loop_Counter, 1).ToShortDateString
            Dim _Month_End_ As String = DateSerial(CInt(Dts.Variables("User::Flag_File").Value.ToString.Trim.Substring(0, 4)), Month_Loop_Counter + 1, 0).ToShortDateString

            If Month_Loop_Counter = 1 Then
                Query = Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            Else
                Query = Query & ControlChars.CrLf &
                    "Union All" & ControlChars.CrLf & Query_Shell.Replace("_Month_Begin_", _Month_Begin_).Replace("_Month_End_", _Month_End_).Replace("_Organisation_", _Organisation_)
            End If
        Next

        '' Original Code tha selkecty the entire structure of 10 000+ rows
        Query = "SELECT     distinct  * from(" + Query + ") as TreeStucture order by 2,3"


        '' JUST FOR TESTING
        '' Query = "SELECT distinct top(10) *  from(" + Query + ") as TreeStucture order by 2,3"
