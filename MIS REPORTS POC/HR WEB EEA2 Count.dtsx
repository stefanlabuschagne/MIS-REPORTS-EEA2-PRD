        Dts.Variables("User::Query_Detail_TestSQL").Value = Full_Query_Detail
        Dts.Variables("User::Query_Detail").Value = Full_Query_Detail.Trim + "  FOR XML path"
        If (RT = 1) Then '' Query on the Level
            Dts.Variables("User::Destination_Table").Value = Dts.Variables("User::Query_Loop_Data_TargetTable").Value.ToString()
        Else  '' Hierarical Query
            'Dts.Variables("User::Query_Detail_TestSQL").Value = Full_Query_Hierarchical
            'Dts.Variables("User::Query_Detail").Value = Full_Query_Hierarchical.Trim + "  FOR XML path"
            Dts.Variables("User::Destination_Table").Value = Dts.Variables("User::Query_Loop_Data_TargetTable").Value.ToString() + "_H"
        End If

